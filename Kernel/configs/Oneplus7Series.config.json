[
  {
    "kernelSource": {
      "name": "Op7Series_KSU_Nethunter",
      "repo": "https://github.com/LineageOS/android_kernel_oneplus_sm8150",
      "branch": "lineage-23.0",
      "device": "Oneplus7Series",
      "defconfig": "sm8150_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/main/clang-r547379.tar.gz",
        "extract": true,
        "binaryEnv": [
          "clang-r547379/bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": "y"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/internetuser111/AnyKernel3",
        "branch": "op7-nh"
      }
    },
    "enableKernelSU": false,
    "enableLXC": true
  }
]
